{
    "name": "raw-material",
    "version": "0.1.0",
    "description": "program backend to monitor and regulate purchase and transport of raw material used in glass production",
    "author": "Chia-Yu Tsai",
    "private": true,
    "main": "server.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/junior-upgi/rawMaterial.git"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/junior-upgi/rawMaterial/issues"
    },
    "homepage": "https://github.com/junior-upgi/rawMaterial#readme",
    "scripts": {
        "watchify": "watchify -vd -p browserify-hmr -e src/main.js -o dist/build.js",
        "serve": "http-server -o -c 1 -a localhost",
        "dev": "npm-run-all --parallel watchify serve",
        "lint": "gulp lint",
        "test": "karma start karma.conf.js",
        "build": "cross-env NODE_ENV=production browserify -g envify -p [ vueify/plugins/extract-css -o dist/build.css ] -e src/main.js | uglifyjs -c warnings=false -m > dist/build.js"
    },
    "browserify": {
        "transform": [
            "babelify",
            "vueify"
        ]
    },
    "browser": {
        "vue": "vue/dist/vue.common.js"
    },
    "dependencies": {
        "axios": "^0.15.3",
        "body-parser": "^1.15.2",
        "cors": "^2.8.1",
        "express": "^4.14.0",
        "jsonwebtoken": "^7.2.1",
        "ldapjs": "^1.0.1",
        "moment-timezone": "^0.5.11",
        "mssql": "^3.3.0",
        "node-cron": "^1.1.2",
        "request": "^2.79.0",
        "request-promise": "^4.1.1",
        "serve-favicon": "^2.3.2",
        "vue": "^2.1.9",
        "vue-router": "^2.1.1",
        "vuex": "^2.1.1"
    },
    "devDependencies": {
        "babel-core": "^6.0.0",
        "babel-plugin-transform-runtime": "^6.0.0",
        "babel-preset-es2015": "^6.18.0",
        "babel-preset-stage-2": "^6.18.0",
        "babel-runtime": "^6.0.0",
        "babelify": "^7.3.0",
        "browserify": "^13.1.1",
        "browserify-hmr": "^0.3.1",
        "cross-env": "^2.0.0",
        "envify": "^3.4.1",
        "eslint": "^3.12.2",
        "eslint-config-google": "^0.7.1",
        "eslint-config-standard": "^6.2.1",
        "eslint-plugin-html": "^1.7.0",
        "eslint-plugin-promise": "^3.4.0",
        "eslint-plugin-standard": "^2.0.1",
        "gulp": "^3.9.1",
        "gulp-eslint": "^3.0.1",
        "gulp-if": "^2.0.2",
        "gulp-jscs": "^4.0.0",
        "gulp-jscs-stylish": "^1.4.0",
        "gulp-load-plugins": "^1.4.0",
        "gulp-print": "^2.0.1",
        "http-server": "^0.9.0",
        "jasmine-core": "^2.4.1",
        "jscs": "^3.0.7",
        "karma": "^1.2.0",
        "karma-browserify": "^5.1.0",
        "karma-jasmine": "^1.0.2",
        "karma-phantomjs-launcher": "^1.0.0",
        "karma-spec-reporter": "0.0.26",
        "npm-run-all": "^4.0.0",
        "phantomjs-prebuilt": "^2.1.3",
        "proxyquireify": "^3.0.1",
        "uglify-js": "^2.5.0",
        "vueify": "^9.0.0",
        "watchify": "^3.8.0",
        "yargs": "^6.6.0"
    }
}
